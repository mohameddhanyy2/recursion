using namespace std;
#include <bits/stdc++.h>
#define ll long long
using namespace std;

int pow(int m, int n){
    if (n==0){
        return 1;
    }
    if(n%2==0){
        return pow(m*m,n/2);
    }else{
        return m*pow(m*m,(n-1)/2);
    }
}
int main(){
    int n;int k;
    cin >> n >> k;
    cout << pow(n,k);
}



